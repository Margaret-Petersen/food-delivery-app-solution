{
  "expo": {
    "name": "FastGrab Store",
    "slug": "fastgrab-multivendor-restaurant",
    "description": "The official business management tool for all FastGrab vendors. The FastGrab Store app is a centralized platform that puts you in control of your online presence and daily operations. Streamline your workflow, reach more customers, and increase your revenue with a single, intuitive interface.",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./lib/assets/images/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./lib/assets/images/black.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "organization": "ninjas-code-w7",
          "project": "enatega-store-app",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos for license, vehicle plate and profile image. Please allow these to continue using the app."
        }
      ],
      "expo-asset"
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "multivendor.enatega.restaurant",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "$(PRODUCT_NAME) uses your location for features like finding orders nearby and tracking customer orders!",
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIRequiresFullScreen": true
      }
    },
    "android": {
      "versionCode": 55,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "multivendor.enatega.restaurant",
      "adaptiveIcon": {
        "foregroundImage": "./lib/assets/images/appIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "4a84d902-4d69-454e-ae15-d2948abbcca0"
      },
      "router": {}
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "it4chi00"
  }
}
