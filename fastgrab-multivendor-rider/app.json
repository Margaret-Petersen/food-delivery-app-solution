{
  "expo": {
    "name": "Fastgrab Multivendor Rider",
    "description": "The official app for FastGrab delivery partners in Namibia. FastGrab Rider is a powerful tool designed to help you manage deliveries with maximum efficiency. Maximize your earnings with a streamlined, intelligent system that gives you the tools to succeed on your terms.",
    "version": "1.0.2",
    "slug": "fastgrab-rider-multivendor",
    "orientation": "portrait",
    "splash": {
      "image": "./lib/assets/images/black.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "icon": "./lib/assets/images/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "organization": "ninjas-code",
          "project": "enatega-rider-app",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos for license, vehicle plate and profile image. Please allow these to continue using the app."
        }
      ],
      "expo-font",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enatega.multirider",
      "config": {
        "googleMapsApiKey": "AIzaSyCcm7_Wd7uvmC9YnYLu2JHGWPt6z1MaL1E"
      },
      "icon": "./lib/assets/images/icon.png",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "$(PRODUCT_NAME) uses your location for features like finding orders nearby and tracking customer orders!",
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 61,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCcm7_Wd7uvmC9YnYLu2JHGWPt6z1MaL1E"
        }
      },
      "package": "com.enatega.multirider",
      "icon": "./lib/assets/images/appIcon.png",
      "adaptiveIcon": {
        "foregroundImage": "./lib/assets/images/appIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "fc60a80d-5fdd-46a6-b79b-07bfbb9e35a8"
      },
      "router": {}
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "it4chi00"
  }
}
